<?xml version="1.0" encoding="utf-8"?>

<navigation
  xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  android:id="@+id/navigation_user_detail">

  <fragment
    android:id="@+id/fragment_user_detail"
    android:name="com.yuyakaido.android.gaia.user.detail.UserDetailFragment">

    <argument
      android:name="source"
      app:argType="com.yuyakaido.android.gaia.user.detail.UserDetailSource"
      app:nullable="false"
      />

  </fragment>

  <activity
    android:id="@+id/activity_user_detail"
    android:name="com.yuyakaido.android.gaia.user.detail.UserDetailActivity">

    <argument
      android:name="user"
      app:argType="com.yuyakaido.android.gaia.core.domain.entity.User"
      app:nullable="false"
      />

  </activity>

</navigation>